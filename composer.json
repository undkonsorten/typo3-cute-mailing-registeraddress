{
	"name": "undkonsorten/typo3-cute-mailing-registeraddress",
	"type": "typo3-cms-extension",
	"description": "A TYPO3 extension that connects registeraddress and cute-mailing.",
	"homepage": "https://www.undkonsorten.com",
	"authors": [
		{
			"name": "Eike Starkmann",
			"email": "starkmann@undkonsorten.com",
			"homepage": "http://undkonsorten.com",
			"role": "Developer"
		}
	],
	"license": [
		"GPL-2.0+"
	],
	"keywords": [
		"email",
		"newsletter",
		"TYPO3"
	],
	"support": {
		"issues": ""
	},
	"repositories": [
		{
			"type": "vcs",
			"url": ""
		}
	],
	"require": {
		"typo3/cms-backend": "^11.5 || ^12.4",
		"typo3/cms-core": "^11.5 || ^12.4",
		"typo3/cms-extbase": "^11.5 || ^12.4",
		"undkonsorten/typo3-cute-mailing": "^4",
		"afm/registeraddress": "^2.0 || ^3 || ^4 || ^5 || ^6"
	},
	"autoload": {
		"psr-4": {
			"Undkonsorten\\CuteMailingRegisteraddress\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Undkonsorten\\CuteMailingRegisteraddress\\Tests\\": "Tests/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "cute_mailing_registeraddress"
		},
		"branch-alias":{
			"dev-main": "2.2.x-dev"
		}
	}
}
